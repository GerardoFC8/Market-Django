+    font-weight: 600;
+    font-size: 19px;
+    margin-bottom: 3px;
+    text-transform: capitalize;
+}
+
+
+.informacion_producto .extra_prod{
+    color: var(--blanco);
+    font-size: 15px;
+    text-transform: capitalize;
+    margin-bottom: 20px;
+}
+
+.informacion_producto .precio_prod{
+    color: var(--botones);
+    margin-bottom: 5px;
+    font-size: 19px;
+}
+
+.informacion_producto .precio_desc{
+    font-size: 15px;
+    color: var(--botones_hover);
+    text-decoration: line-through;
+}
+
+.envio_prod{
+    display: flex;
+    margin-top: 20px;
+}
+
+.envio_prod img{
+    margin-right: 10px;
+}
+
