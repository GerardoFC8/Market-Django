+    /*5px 5px 15px 5px rgba(0,0,0,0.37);*/
+
+    /*background-color: var(--producto_hover);*/
+}
+
+.contenedor_img_prod{
+    width: 100%;
+    height: 122px;
+    background: #fff;
+    border-radius: 10px;
+    margin-bottom: 20px;
+    display: flex;
+    justify-content: center;
+    align-items: center;
+    padding: 12px 0px;
+    overflow: hidden;
+}
+
+.contenedor_img_prod img{
+    width: 70%;
+    transition: all 800ms;
+}
+
+.contenedor_img_prod img:hover{
+    width: 100%;
+}
+
+.icon{
+    width: 20px;
+    padding: 0 0 20px 0;
+}
+
+.boton_agregar{
+    display: block;
+    background-color: var(--botones);
+    border-radius: 50px;
+    text-align: center;    padding: 10px;
+    text-decoration: none;
+    color: white;
+    text-transform: capitalize;
+    margin-top: 12px;
+    font-size: 15px;
+    transition: all 600ms;
+}
+
+.boton_agregar:hover{
+    background-color: var(--botones_hover);
+}
+
+
+
+
